Private Sub tmrLoading_Timer()
    frmMain.Show
    Unload Me
End Sub

Private Sub trmLoading_Timer()
    If lblCompany.Caption = "Loading" Then
        lblCompany.Caption = "Loading."
    ElseIf lblCompany.Caption = "Loading." Then
        lblCompany.Caption = "Loading.."
    ElseIf lblCompany.Caption = "Loading.." Then
        lblCompany.Caption = "Loading..."
    ElseIf lblCompany.Caption = "Loading..." Then
        lblCompany.Caption = "Loading...."
    ElseIf lblCompany.Caption = "Loading...." Then
        lblCompany.Caption = "Loading....."
    Else
        lblCompany.Caption = "Loading"
    End If
        
End Sub
